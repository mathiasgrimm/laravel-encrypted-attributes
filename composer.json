{
    "name": "mathiasgrimm/laravel-encrypted-attributes",
    "description": "Encrypted and safe-by-default Laravel Model attributes",
    "license": ["MIT"],
    "tags": ["laravel", "model", "encrypt", "decrypt", "attribute"],
    "authors": [
        {
            "name": "Mathias Grimm",
            "email": "mathiasgrimm@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MathiasGrimm\\LaravelEncryptedAttributes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.11",
        "orchestra/testbench": "^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    }
}
